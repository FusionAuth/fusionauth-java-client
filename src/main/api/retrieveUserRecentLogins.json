{
  "uri": "/api/user/recent-login",
  "comments": [
    "Retrieves the last number of login records for a user."
  ],
  "method": "get",
  "methodName": "retrieveUserRecentLogins",
  "successResponse": "RecentLoginResponse",
  "errorResponse": "Errors",
  "params": [
    {
      "name": "userId",
      "comments": [
        "The Id of the user."
      ],
      "type": "urlParameter",
      "parameterName": "userId",
      "javaType": "UUID"
    },
    {
      "name": "offset",
      "comments": [
        "The initial record. e.g. 0 is the last login, 100 will be the 100th most recent login."
      ],
      "type": "urlParameter",
      "parameterName": "offset",
      "javaType": "int"
    },
    {
      "name": "limit",
      "comments": [
        "(Optional, defaults to 10) The number of records to retrieve."
      ],
      "type": "urlParameter",
      "parameterName": "limit",
      "javaType": "Integer"
    }
  ]
}